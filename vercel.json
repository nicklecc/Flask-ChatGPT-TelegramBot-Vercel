{
	    "builds": [{
	        "src": "main.py",
	        "use": "@vercel/python",
	        "config": { "maxLambdaSize": "15mb" }
	    }],
	    "routes": [
	        {
	            "src": "/(.*)",
	            "dest": "main.py"
	        }
	    ]
}
{
  "functions": {
    "api": {
      "memory": 128,
      "timeout": 30
    }
  }
}
